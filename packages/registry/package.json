{
  "name": "@neuledge/registry",
  "version": "0.0.0",
  "private": true,
  "description": "Registry infrastructure for building and publishing context packages",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "registry": "./dist/cli.js",
    "test-registry": "./dist/test-registry.js"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "rimraf --glob dist/** && tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test-registry": "tsx src/test-registry.ts",
    "lint": "npx biome ci --error-on-warnings",
    "fix": "npx biome check --write"
  },
  "author": "Neuledge",
  "license": "Apache-2.0",
  "dependencies": {
    "@neuledge/context": "workspace:*",
    "commander": "^14.0.0",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^24.9.1",
    "rimraf": "^6.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.3"
  },
  "tsx": {
    "require": [
      "tsconfig-paths/register"
    ]
  }
}
